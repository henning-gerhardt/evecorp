<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDefault>
			<ROOT>
				<TCEforms>
					<sheetTitle>Options</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.corporation>
						<TCEforms>
							<label>Choose corporation restriction</label>
							<config>
								<type>select</type>
								<foreign_table>tx_evecorp_domain_model_corporation</foreign_table>
								<!-- TODO: add where restriction to current used record storage pids -->
								<foreign_table_where>ORDER BY corporation_name</foreign_table_where>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.corporation>
					<settings.showCorporationJoinDate>
						<TCEforms>
							<label>Show characters corporation join date</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.showCorporationJoinDate>
					<settings.showApiKeyState>
						<TCEforms>
							<label>Show api key expire state</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.showApiKeyState>
					<settings.showLoginUser>
						<TCEforms>
							<label>Show login user name</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.showLoginUser>
				</el>
			</ROOT>
		</sDefault>
	</sheets>
</T3DataStructure>
